<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="aQuincheS3.Page1">
    <ContentPage.Content>
        <StackLayout Style="{StaticResource stackLayoutStyle}">
            <Label Text="SEMANA3" FontSize="18" HorizontalTextAlignment="Center" TextColor="Green" />
            <Entry x:Name="txtDatoUno" Placeholder="Dato uno"/>
            <Entry x:Name="txtDatoDos" Placeholder="Dato Dos" Keyboard="Numeric"/>
            <Button x:Name="btnAbrir" Text="ABRIR" Clicked="btnAbrir_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>